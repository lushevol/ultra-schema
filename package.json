  "scripts": {
    "build": "rsbuild build",
    "build:wp": "webpack build",
    "build:rs": "rspack build",
    "dev": "rsbuild dev --open",
    "dev:wp": "webpack serve",
    "dev:rs": "rspack serve",
    "codegen-graphql": "graphql-codegen --config codegen.yml",
    "codegen-openapi": "npx @rtk-query/codegen-openapi rtk-query-openapi.ts",
    "json2graphql2": "json-schema-to-graphql src/json-schema-form/ssi-form-json-schema.json src/json-schema-form/ssi-form.generated.graphql",
    "json2graphql": "cat src/json-schema-form/ssi-form-json-schema.json | npx json-to-simple-graphql-schema > src/json-schema-form/ssi-form.generated.graphql",
    "json2ts": "json2ts -i src/json-schema-form/ssi-form-json-schema.json -o src/json-schema-form/ssi-form-types.generated.ts",
    "json2mock": "generate-json src/json-schema-form/ssi-form-json-schema.json src/json-schema-form/ssi-form-mock.generated.json",
    "format": "biome format --write",
    "check": "biome check --write",
    "preview": "rsbuild preview",
    "depcheck": "npx depcheck",
    "bump": "npx taze"
  },
